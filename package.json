{
  "name": "03-api-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "prestart:dev": "docker compose up -d",
    "start:dev": "tsx watch --env-file=.env ./src/server.ts",
    "dev": "tsx watch --env-file=.env ./src/server.ts",
    "start": "node build/server.js",
    "build": "tsup ./src --out-dir build",
    "lint": "eslint src --ext .ts --fix",
    "test:create-environment-prisma": "npm link ./prisma/vitest-environment-prisma",
    "test:install-environment-prisma": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-environment-prisma test:install-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http --test-timeout=12000",
    "test:e2e:watch": "vitest --dir src/http --test-timeout=12000",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --api 9572"
  },
  "keywords": [],
  "author": "Vinicius <viniciuferreira32@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^20.11.17",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.5.3",
    "@vitest/ui": "1.5.3",
    "dotenv": "16.4.5",
    "eslint": "8.56.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.9.1",
    "supertest": "7.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.5.3"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.1",
    "@prisma/client": "5.9.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.11",
    "fastify": "^4.26.1",
    "zod": "3.22.4"
  }
}
