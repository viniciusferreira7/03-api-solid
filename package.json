{
  "name": "03-api-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusferreira7/03-api-solid.git"
  },
  "keywords": [],
  "author": "Vinicius <viniciuferreira32@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viniciusferreira7/03-api-solid/issues"
  },
  "homepage": "https://github.com/viniciusferreira7/03-api-solid#readme",
  "dependencies": {
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.3.1",
    "@types/node": "^20.6.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "fastify": "^4.23.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.9",
    "typescript": "^5.2.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.4",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "npm-run-all": "4.1.5",
    "prisma": "5.3.1",
    "supertest": "6.3.4",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  }
}
